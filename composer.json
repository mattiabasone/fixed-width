{
    "name": "mattiabasone/fixed-width",
    "description": "Library for parsing/generating fixed width files",
    "keywords": ["fixed-width", "positional", "file", "parser"],
    "type": "library",
    "autoload": {
        "psr-4": {
            "MattiaBasone\\FixedWidth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattiaBasone\\FixedWidth\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mattia Basone",
            "email": "mattia.basone@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "symfony/polyfill-php83": "^1.28"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.9",
        "friendsofphp/php-cs-fixer": "^3.15",
        "php-coveralls/php-coveralls": "^2.5.3"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "analyse": "@php vendor/bin/psalm"
    }
}
